    private void formInternalFrameOpened(javax.swing.event.InternalFrameEvent evt) {                                         
//        getAllDivisions();
     //   getAllLecturers();
        

         ////2- Fill Groups JCombo        
         jComboBoxGroups.insertItemAt("", 0);
        jComboBoxGroups.setSelectedIndex(0);
        jComboBoxGroups.removeAllItems();

        LinkedHashMap<String, Integer> groupsHM = GroupHandler.getAllGroupsMap();
        for (String groupName:  groupsHM.keySet()) {
            jComboBoxGroups.addItem(groupName);
            System.out.println("Add : " + groupName);
        }

         ////2- Fill Days JCombo        
        jComboBoxDays.insertItemAt("", 0);
        jComboBoxDays.setSelectedIndex(0);
        jComboBoxDays.removeAllItems();

        LinkedHashMap<String, Integer> daysHM = DayHandler.getAllDaysMap();
        for (String dayName : daysHM.keySet()) {
            jComboBoxDays.addItem(dayName);
            System.out.println("Add : " + dayName);
        }
        
        
        
         ////2- Fill Periods JCombo        
        jComboBoxPeriods.insertItemAt("", 0);
        jComboBoxPeriods.setSelectedIndex(0);
        jComboBoxPeriods.removeAllItems();

        LinkedHashMap<String, Integer> periodsHM = PeriodHandler.getAllPeriodsMap();
        for (String periodName:  periodsHM.keySet()) {
            jComboBoxPeriods.addItem(periodName);
            System.out.println("Add : " + periodName);
        }
        
              ////2- Fill Periods JCombo        
        jComboBoxAddClass.insertItemAt("", 0);
        jComboBoxAddClass.setSelectedIndex(0);
        jComboBoxAddClass.removeAllItems();

        LinkedHashMap<String, Integer> classesHM = ClassHandler.getAllClassesMap();
        for (String className:  classesHM.keySet()) {
            jComboBoxAddClass.addItem(className);
            System.out.println("Add : " + className);
        }
    }                                        

    private void jTablePeriodsGroupsMouseClicked(java.awt.event.MouseEvent evt) {                                                 
              
        Integer periodId = (Integer) jTablePeriodsGroups.getValueAt(jTablePeriodsGroups.getSelectedRow(), 0);
        jTextFieldSelectedPeriodId.setText(String.valueOf(periodId));

        String periodName=  (String) jTablePeriodsGroups.getValueAt(jTablePeriodsGroups.getSelectedRow(), 1);
        jTextFieldSelectedPeriodName.setText(String.valueOf(periodName));

    }                                                

    private void jTableClassesMouseClicked(java.awt.event.MouseEvent evt) {                                           
  Integer classId = (Integer) jTableClasses.getValueAt(jTableClasses.getSelectedRow(), 0);
        jTextFieldSelectedClassId.setText(String.valueOf(classId));

        String className=  (String) jTableClasses.getValueAt(jTableClasses.getSelectedRow(), 1);
        jTextFieldSelectedClassName.setText(String.valueOf(className));

    }                                          

    private void jButtonFilterByGroupActionPerformed(java.awt.event.ActionEvent evt) {                                                     
               String groupName = (String) jComboBoxGroups.getSelectedItem();
                  Group group = GroupHandler.getGroupByName(groupName);

                       String dayName =  (String) jComboBoxDays.getSelectedItem();
                       Day day = DayHandler.getDayByName(dayName);
                       
                
                        // fill period table with this filterations
                       getPeriodsByGroupDay(group, day);
                       
                  
                       // fill rooms of this Group
                       getClassesByGroup(group);
                       
                       // set fields
                       jTextFieldGroupSelectedName.setText(group.getGroupName());
                       jTextFieldSelectedGroupNamePrd.setText(group.getGroupName());
                       jTextFieldSelectedDayName.setText(day.getDayName());
                  
    }                                                    

    private void jButtonAddClassActionPerformed(java.awt.event.ActionEvent evt) {                                                
       int optionChoosen = JFrameMaster.checkConfirm();
        if (optionChoosen == JOptionPane.YES_OPTION) {
            // 0. read Fields
   Group group = GroupHandler.getGroupByName( jTextFieldGroupSelectedName.getText() ) ;

           String className = (String) jComboBoxAddClass.getSelectedItem();
                   Classes classes = ClassHandler.getClassByName(className);
            
            // 1. Add row to DB
            ClassHandler.assignClassToGroup(classes, group);

            // 2. Add row to JTable
            DefaultTableModel tm = (DefaultTableModel) jTableClasses.getModel();
            Object[] rowData = {classes.getClassId(), classes.getClassName() };

            tm.addRow(rowData);

            /// refresh
    //        tm.setRowCount(0);
  //          getAllDivisions();

            // 3. Empty fields
//            emptyFieldsDivisions();

            // 4. show Save Msg
            JOptionPane.showMessageDialog(null, "Data Saved Done", "Saving..", JOptionPane.INFORMATION_MESSAGE);
        }
    }                                               

    private void jButtonAddPeriodActionPerformed(java.awt.event.ActionEvent evt) {                                                 
 
        
        int optionChoosen = JFrameMaster.checkConfirm();
        if (optionChoosen == JOptionPane.YES_OPTION) {
            // 0. read Fields
   Group group = GroupHandler.getGroupByName( jTextFieldSelectedGroupNamePrd.getText() );

           String periodName = (String) jComboBoxPeriods.getSelectedItem();
                   Period period = PeriodHandler.getPeriodByName(periodName);
            
            // 1. Add row to DB
            PeriodHandler.assignPeriodToGroup(period, group);

            // 2. Add row to JTable
            DefaultTableModel tm = (DefaultTableModel) jTablePeriodsGroups.getModel();
            Object[] rowData = {period.getPeriodId(), period.getPeriodName(),
            period.getStartTime().getTimeText(), period.getEndTime().getTimeText()};

            tm.addRow(rowData);

            /// refresh
    //        tm.setRowCount(0);
  //          getAllDivisions();

            // 3. Empty fields
//            emptyFieldsDivisions();

            // 4. show Save Msg
            JOptionPane.showMessageDialog(null, "Data Saved Done", "Saving..", JOptionPane.INFORMATION_MESSAGE);
        }
     
    }                                                

    private void jButtonDeleteClassActionPerformed(java.awt.event.ActionEvent evt) {                                                   

        int optionChoosen = JFrameMaster.checkConfirm();
        if (optionChoosen == JOptionPane.YES_OPTION) {
            //1. read user id
           Group group = GroupHandler.getGroupByName(jTextFieldGroupSelectedName.getText());
            Classes classes = ClassHandler.getClassByName(jTextFieldSelectedClassName.getText());
            
      
            
    //2. del from db
            ClassHandler.deassignClassFromGroup(group, classes);

            // 3. remove From JTable
            DefaultTableModel tm = (DefaultTableModel) jTableClasses.getModel();
            tm.removeRow(jTableClasses.getSelectedRow());

            // 4. empty Fields
    //        emptyFieldsLecturers();
  //          LogHandler.insertLog(9, 3, "Delete Consolate Ok - Consolate  : " + logDeletedConsolateName);
        // reset
            jTextFieldSelectedClassId.setText(null);
            jTextFieldSelectedClassName.setText(null);
        }

    }                                                  

    private void jButtonDeletePeriodActionPerformed(java.awt.event.ActionEvent evt) {                                                    
   int optionChoosen = JFrameMaster.checkConfirm();
        if (optionChoosen == JOptionPane.YES_OPTION) {
            //1. read user id
           Group group = GroupHandler.getGroupByName(jTextFieldGroupSelectedName.getText());
            Period period = PeriodHandler.getPeriodByName(jTextFieldSelectedPeriodName.getText());
            
    //2. del from db
            PeriodHandler.deassignPeriodFromGroup(group, period);

            // 3. remove From JTable
            DefaultTableModel tm = (DefaultTableModel) jTablePeriodsGroups.getModel();
            tm.removeRow(jTablePeriodsGroups.getSelectedRow());

            // 4. empty Fields
    //        emptyFieldsLecturers();
  //          LogHandler.insertLog(9, 3, "Delete Consolate Ok - Consolate  : " + logDeletedConsolateName);
      // reset
              jTextFieldSelectedPeriodId.setText(null);
              jTextFieldSelectedPeriodName.setText(null);
        }
      
    }                                                   

    public void getPeriodsByGroupDay(Group group, Day day) {
     ArrayList<Period> periodsList = PeriodHandler.getPeriodsByGroupDay(group, day);
        // 2. Add Rows to JTable
        DefaultTableModel tm = (DefaultTableModel) jTablePeriodsGroups.getModel();
   tm.setRowCount(0);
        for (Period period : periodsList) {
            Object[] rowData = {period.getPeriodId(), period.getPeriodName(),
                period.getStartTime().getTimeText(), period.getEndTime().getTimeText()};

            tm.addRow(rowData);
        }


    }

     public void getClassesByGroup(Group group) {

    ArrayList<Classes> classesList = ClassHandler.getClassesByGroup(group);
        // 2. Add Rows to JTable
        DefaultTableModel tm = (DefaultTableModel) jTableClasses.getModel();
          tm.setRowCount(0);
        for (Classes classes : classesList) {
            Object[] rowData = {classes.getClassId(), classes.getClassName()};
            tm.addRow(rowData);
        }


    }

