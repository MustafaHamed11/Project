    private void formWindowOpened(java.awt.event.WindowEvent evt) {                                  
        /// Show Login J Internal Frame
        JInternalFrameLogin loginForm = new JInternalFrameLogin(this);
        jDesktopPaneMaster.add(loginForm);
        loginForm.setVisible(true);
        this.setInternalFrameLocationCentered(loginForm);

        // Menu Invisible
        this.changeMenuVisibilty(false);

    }                                 

    private void jMenuItemUserMgtActionPerformed(java.awt.event.ActionEvent evt) {                                                 

        JInternalFrameUsersMgt userMgtForm = new JInternalFrameUsersMgt(this);
        jDesktopPaneMaster.add(userMgtForm);
        userMgtForm.setVisible(true);
        setInternalFrameLocationCentered(userMgtForm);
    }                                                

    private void jMenuItemDivisionsGroupsActionPerformed(java.awt.event.ActionEvent evt) {                                                         
