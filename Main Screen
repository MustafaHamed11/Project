        divisionsGroups.setVisible(true);
        setInternalFrameLocationCentered(divisionsGroups);
    }                                                        

    private void jMenuItemSetupConsolateKanaatActionPerformed(java.awt.event.ActionEvent evt) {                                                              
        JInternalFrameLookupsLecturersDepts lookupsForm = new JInternalFrameLookupsLecturersDepts(this);
        jDesktopPaneMaster.add(lookupsForm);
        lookupsForm.setVisible(true);
        setInternalFrameLocationCentered(lookupsForm);
    }                                                             

    private void jMenuItemLogoutActionPerformed(java.awt.event.ActionEvent evt) {                                                
        int optionChoosen = checkConfirm();
        if (optionChoosen == JOptionPane.YES_OPTION) {

            jDesktopPaneMaster.removeAll();
            jDesktopPaneMaster.updateUI();
            JInternalFrameLogin.setLoggedUser(new User());
            JInternalFrameLogin loginForm = new JInternalFrameLogin(this);
            jDesktopPaneMaster.add(loginForm);
            loginForm.setVisible(true);
            //   loginForm.setLocation(400, 300);
            setInternalFrameLocationCentered(loginForm);
            this.setTitle("Zag Sched Application v 1.0.0");
                LogHandler.insertLog(5, 1, "Logout Ok");
            changeMenuVisibilty(false);
            
                 // Set Log
        }
    }                                               

    private void jMenuItemExitActionPerformed(java.awt.event.ActionEvent evt) {                                              
        int optionChoosen = checkConfirm();
        if (optionChoosen == JOptionPane.YES_OPTION) {
            System.exit(0);
        }
    }                                             

