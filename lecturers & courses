    private void formInternalFrameOpened(javax.swing.event.InternalFrameEvent evt) {                                         
        getAllCourses();
        
        getLecturersByCourseId(-1); // no lecturers

        
         ////2- Fill Lecturers JCombo
        
         jComboBoxLecturer.insertItemAt("", 0);
         jComboBoxLecturer.setSelectedIndex(0);
         jComboBoxLecturer.removeAllItems();

         LinkedHashMap<String, Integer> lecturersHM = LecturerHandler.getAllLecturersMap();
         for (String lecturerName : lecturersHM.keySet()) {
         jComboBoxLecturer.addItem(lecturerName);
         System.out.println("Add : " + lecturerName);
         }
        
         
         
    }                                        

    private void jTableCourseMouseClicked(java.awt.event.MouseEvent evt) {                                          

        Integer courseId = (Integer) jTableCourse.getValueAt(jTableCourse.getSelectedRow(), 0);
        String courseName = (String) jTableCourse.getValueAt(jTableCourse.getSelectedRow(), 1);

//        JOptionPane.showMessageDialog(null, "course choose = " + courseId + " " + courseName);
        jTextFieldSelectedCourseId.setText(String.valueOf(courseId));
        jTextFieldSelectedCourseName.setText(courseName);
        getLecturersByCourseId(courseId);
//         jTextFieldCourseId.setText(courseId.toString());
//         jTextFieldCourseName.setText(courseName);

         //   jComboBoxDivision.setSelectedItem(DivisionHandler.getDivisionByName(divisionName).getDivisionName());
        //empty fields
        jTextFieldSelectedLecturerId.setText(null);
        jTextFieldSelectedLecturerName.setText(null);
    }                                         

    private void jTableLecturersMouseClicked(java.awt.event.MouseEvent evt) {                                             
        
        Integer lecturerId = (Integer) jTableLecturers.getValueAt(jTableLecturers.getSelectedRow(), 0);
                jTextFieldSelectedLecturerId.setText(String.valueOf(lecturerId));
                
             String lecturerName = (String) jTableLecturers.getValueAt(jTableLecturers.getSelectedRow(), 1);
                jTextFieldSelectedLecturerName.setText(String.valueOf(lecturerName));
                         
        
        
        
        
    }                                            

    private void jButtonAssignLecturertoCrsActionPerformed(java.awt.event.ActionEvent evt) {                                                           

        int optionChoosen = JFrameMaster.checkConfirm();
        if (optionChoosen == JOptionPane.YES_OPTION) {
            // 0. read Fields
           Course course = CourseHandler.getCourseById( Integer.parseInt( jTextFieldSelectedCourseId.getText() ) );

           String lecturerName = (String) jComboBoxLecturer.getSelectedItem();
                   Lecturer lecturer = LecturerHandler.getLecturerByName(lecturerName);
            
            
            

            // 1. Add row to DB
            LecturerHandler.assignLecturerToCourse(lecturer, course);

            // 2. Add row to JTable
            DefaultTableModel tm = (DefaultTableModel) jTableLecturers.getModel();
            Object[] rowData = {lecturer.getLecturerId(), lecturer.getLecturerName(), lecturer.getLecturerPosition().getLecturerPosName()};

            tm.addRow(rowData);

            /// refresh
    //        tm.setRowCount(0);
  //          getAllDivisions();

            // 3. Empty fields
//            emptyFieldsDivisions();

            // 4. show Save Msg
            JOptionPane.showMessageDialog(null, "Data Saved Done", "Saving..", JOptionPane.INFORMATION_MESSAGE);
        }
    }                                                          

    private void jButtonDeAssignLecturertoCrsActionPerformed(java.awt.event.ActionEvent evt) {                                                             
        int optionChoosen = JFrameMaster.checkConfirm();
        if (optionChoosen == JOptionPane.YES_OPTION) {
            //1. read user id
            int lecturerId = Integer.parseInt(jTextFieldSelectedLecturerId.getText());
            int courseId = Integer.parseInt(jTextFieldSelectedCourseId.getText());
            
            Lecturer lecturer = LecturerHandler.getLecturerById(lecturerId);
            Course course = CourseHandler.getCourseById(courseId);
            
            
    //2. del from db
            LecturerHandler.deassignLecturerFromCourse(lecturer, course);

            // 3. remove From JTable
            DefaultTableModel tm = (DefaultTableModel) jTableLecturers.getModel();
            tm.removeRow(jTableLecturers.getSelectedRow());

            // 4. empty Fields
    //        emptyFieldsLecturers();
  //          LogHandler.insertLog(9, 3, "Delete Consolate Ok - Consolate  : " + logDeletedConsolateName);
        }

    }                                                            

    public void getLecturersByCourseId(int courseId) {
    ArrayList<Lecturer> lecturersList = LecturerHandler.getLecturersByCourseId(courseId);
        // 2. Add Rows to JTable
        DefaultTableModel tm = (DefaultTableModel) jTableLecturers.getModel();
   tm.setRowCount(0);
        for (Lecturer lecturer : lecturersList) {
            Object[] rowData = {lecturer.getLecturerId(), lecturer.getLecturerName(),
                lecturer.getLecturerPosition().getLecturerPosName()};

            tm.addRow(rowData);
        }

    }
    
    public void getAllCourses() {

// 1. Get All Software From DB
        ArrayList<Course> coursesList = CourseHandler.getAllCourses();
        // 2. Add Rows to JTable
        DefaultTableModel tm = (DefaultTableModel) jTableCourse.getModel();

        for (Course course : coursesList) {
            Object[] rowData = {course.getCourseId(), course.getCourseName()};
            tm.addRow(rowData);
        }

    }

    public void getAllLecturers() {
        // 1. Get All Software From DB
        ArrayList<Lecturer> lecturersList = LecturerHandler.getAllLecturers();
        // 2. Add Rows to JTable
        DefaultTableModel tm = (DefaultTableModel) jTableLecturers.getModel();

         
        for (Lecturer lecturer : lecturersList) {
            Object[] rowData = {lecturer.getLecturerId(), lecturer.getLecturerName(),
                lecturer.getLecturerPosition().getLecturerPosName()};

            tm.addRow(rowData);
        }
    }

