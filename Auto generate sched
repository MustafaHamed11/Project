    private void jButtonLectureCreateActionPerformed(java.awt.event.ActionEvent evt) {                                                     
 int optionChoosen = JOptionPane.showConfirmDialog(null, "Are You sure ?", "Confirm Message", JOptionPane.YES_NO_CANCEL_OPTION, JOptionPane.WARNING_MESSAGE);
 
    if(optionChoosen == JOptionPane.YES_OPTION){
        // call procedure
          LectureHandler.callLecturesProcedure();
        
        System.out.println("call procedure");
    }
    }                                                    

    private void jButtonRemoveScheduleActionPerformed(java.awt.event.ActionEvent evt) {                                                      
int optionChoosen = JOptionPane.showConfirmDialog(null, "Are You sure ?", "Confirm Message", JOptionPane.YES_NO_CANCEL_OPTION, JOptionPane.WARNING_MESSAGE);
 
    if(optionChoosen == JOptionPane.YES_OPTION){
        // call procedure
          LectureHandler.callRemovelectsSections();
        System.out.println("call procedure");
    }
    }                                                     

